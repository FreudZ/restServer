{
  "name": "kigkonsult/restserver",
  "description": "rest services made easy, restServer, based on PSR http message, builtin serializers and decoders",
  "keywords": [
    "rest",
    "server",
    "service",
    "psr",
    "psr-7",
    "http",
    "http-message",
    "request",
    "response"
  ],
  "type": "library",
  "license": "AGPL-1.0",
  "homepage": "https://github.com/iCalcreator/restserver",
    "authors": [
        {
            "name": "Kjell-Inge Gustafsson",
            "homepage": "https://kigkonsult.se/RestServer/index.php"
        }
    ],
  "support": {
    "issues": "https://github.com/iCalcreator/restserver/issues",
    "source": "https://github.com/iCalcreator/restServer"
  },
  "config": {
      "sort-packages": true
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "fig/http-message-util": "^1.0",
    "nikic/fast-route": "^1.2.0",
    "psr/http-message": "^1.0",
    "zendframework/zend-diactoros": "1.7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1.0"
  },
  "autoload": {
    "psr-4": {
      "Kigkonsult\\RestServer\\": "src/"
    },
    "files": [
      "src/autoload.php"
    ]
  }
}
